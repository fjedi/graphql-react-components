{
  "name": "@fjedi/graphql-react-components",
  "version": "11.0.4",
  "description": "A set of helpers for managing apollo-graphql inside react-app",
  "browser": "dist/browser",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./browser": "./dist/browser.mjs",
    "./browser.native": "./dist/browser.native.js",
    "./server": "./dist/server.js"
  },
  "private": false,
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watchAll",
    "clean": "rimraf dist",
    "coverage": "jest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "build": "npm run clean && rollup -c",
    "start": "rollup -c -w",
    "format": "prettier --write \"src/**/*.tsx\"",
    "prepare": "npm run build && husky install",
    "prepublishOnly": "npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fjedi/graphql-react-components.git"
  },
  "keywords": [
    "fjedi",
    "graphql",
    "apollo",
    "react",
    "typescript"
  ],
  "author": "Alexander Radyushin <r.alex.andr@ya.ru> (https://fjedi.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fjedi/graphql-react-components/issues"
  },
  "homepage": "https://github.com/fjedi/graphql-react-components#readme",
  "dependencies": {
    "@apollo/client": "3.6.9",
    "@types/apollo-upload-client": "17.0.1",
    "apollo-link-debounce": "3.0.0",
    "apollo-upload-client": "17.0.0",
    "core-js": "3.25.0",
    "crypto-hash": "2.0.1",
    "graphql": "16.6.0",
    "graphql-ws": "5.10.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "22.0.2",
    "@rollup/plugin-node-resolve": "14.0.0",
    "@rollup/plugin-replace": "4.0.0",
    "@types/jest": "29.0.0",
    "@types/lodash": "4.17.12",
    "@types/node": "18.7.15",
    "@types/react": "18.0.18",
    "@typescript-eslint/eslint-plugin": "5.36.2",
    "@typescript-eslint/parser": "5.36.2",
    "antd": "4.23.0",
    "babel-core": "6.26.3",
    "babel-runtime": "6.26.0",
    "dayjs": "1.11.5",
    "eslint": "8.22.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-compat": "4.0.2",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.31.7",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-security": "1.5.0",
    "husky": "8.0.1",
    "i18next": "21.9.1",
    "jest": "28.1.3",
    "lint-staged": "13.0.3",
    "lodash": "4.17.21",
    "prettier": "2.7.1",
    "prop-types": "15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "11.18.5",
    "rimraf": "3.0.2",
    "rollup": "2.79.0",
    "rollup-plugin-node-polyfills": "0.2.1",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "rollup-plugin-terser": "7.0.2",
    "rollup-plugin-typescript2": "0.33.0",
    "ts-jest": "28.0.8",
    "typescript": "4.8.2"
  },
  "peerDependencies": {
    "@types/lodash": ">=4.14.184",
    "antd": ">=4.21.3",
    "dayjs": "^1.11.5",
    "i18next": ">=21.9.1",
    "lodash": ">=4.17.21",
    "prop-types": ">=15.8.1",
    "react": ">=18.2.0",
    "react-dom": ">=18.2.0",
    "react-i18next": ">=11.18.5",
    "react-router-dom": ">=6.3.0"
  },
  "lint-staged": {
    "*.(js|jsx|ts|tsx)": [
      "npm run lint:fix",
      "git add"
    ]
  }
}