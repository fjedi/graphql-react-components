{
  "name": "@fjedi/graphql-react-components",
  "version": "8.1.17",
  "description": "A set of helpers for managing apollo-graphql inside react-app",
  "browser": "dist/browser",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "private": false,
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watchAll",
    "clean": "rimraf dist",
    "coverage": "jest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "build": "npm run clean && rollup -c",
    "start": "rollup -c -w",
    "format": "prettier --write \"src/**/*.tsx\"",
    "prepare": "npm run build && husky install",
    "prepublishOnly": "npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fjedi/graphql-react-components.git"
  },
  "keywords": [
    "fjedi",
    "graphql",
    "apollo",
    "react",
    "typescript"
  ],
  "author": "Alexander Radyushin <r.alex.andr@ya.ru> (https://fjedi.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fjedi/graphql-react-components/issues"
  },
  "homepage": "https://github.com/fjedi/graphql-react-components#readme",
  "dependencies": {
    "@apollo/client": "3.4.16",
    "@fjedi/errors": "1.1.4",
    "@types/apollo-upload-client": "14.1.0",
    "apollo-link-debounce": "3.0.0",
    "apollo-upload-client": "16.0.0",
    "apollo-utilities": "1.3.4",
    "core-js": "3.19.0",
    "cross-fetch": "3.1.4",
    "crypto-hash": "1.3.0",
    "graphql": "15.7.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "21.0.1",
    "@rollup/plugin-node-resolve": "13.0.6",
    "@types/jest": "27.0.2",
    "@types/lodash": "4.14.175",
    "@types/node": "16.11.6",
    "@types/react": "17.0.31",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "antd": "4.16.13",
    "babel-core": "6.26.3",
    "babel-runtime": "6.26.0",
    "eslint": "7.32.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-airbnb-typescript": "14.0.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-compat": "3.13.0",
    "eslint-plugin-import": "2.25.2",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.26.1",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-security": "1.4.0",
    "husky": "7.0.4",
    "i18next": "21.3.3",
    "jest": "27.2.4",
    "lint-staged": "11.2.6",
    "lodash": "4.17.21",
    "prettier": "2.4.1",
    "prop-types": "15.7.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-i18next": "11.12.0",
    "rimraf": "3.0.2",
    "rollup": "2.58.0",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "rollup-plugin-terser": "7.0.2",
    "rollup-plugin-typescript2": "0.30.0",
    "subscriptions-transport-ws": "0.9.19",
    "ts-jest": "27.0.7",
    "typescript": "4.4.4"
  },
  "peerDependencies": {
    "@types/lodash": "^4.14.173",
    "antd": "^4.16.13",
    "i18next": "^20.6.1 || ^21.0.0",
    "lodash": "^4.17.21",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.12.0",
    "subscriptions-transport-ws": "^0.9.19"
  },
  "lint-staged": {
    "*.(js|jsx|ts|tsx)": [
      "npm run lint:fix",
      "git add"
    ]
  }
}
