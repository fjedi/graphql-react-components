{
  "name": "@fjedi/graphql-react-components",
  "version": "7.0.2",
  "description": "A set of helpers for managing apollo-graphql inside react-app",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "private": false,
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watchAll",
    "clean": "rimraf lib",
    "coverage": "jest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "build": "rollup -c",
    "start": "rollup -c -w",
    "format": "prettier --write \"src/**/*.tsx\"",
    "prepare": "npm run build && husky install",
    "prepublishOnly": "npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fjedi/graphql-react-components.git"
  },
  "keywords": [
    "fjedi",
    "graphql",
    "apollo",
    "react",
    "typescript"
  ],
  "author": "Alexander Radyushin <r.alex.andr@ya.ru> (https://fjedi.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fjedi/graphql-react-components/issues"
  },
  "homepage": "https://github.com/fjedi/graphql-react-components#readme",
  "dependencies": {
    "@apollo/client": "3.4.7",
    "@fjedi/errors": "1.1.3",
    "@types/apollo-upload-client": "14.1.0",
    "apollo-link-debounce": "3.0.0",
    "apollo-link-persisted-queries": "0.2.2",
    "apollo-progressive-fragment-matcher": "1.0.8",
    "apollo-upload-client": "16.0.0",
    "apollo-utilities": "1.3.4",
    "graphql": "15.5.1"
  },
  "devDependencies": {
    "@types/jest": "27.0.0",
    "@types/lodash": "4.14.172",
    "@types/node": "14.17.9",
    "@types/react": "17.0.17",
    "@typescript-eslint/eslint-plugin": "4.29.1",
    "@typescript-eslint/parser": "4.29.1",
    "antd": "4.16.11",
    "babel-core": "6.26.3",
    "babel-runtime": "6.26.0",
    "eslint": "7.32.0",
    "eslint-config-airbnb-typescript": "12.3.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-compat": "3.12.0",
    "eslint-plugin-import": "2.24.0",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-security": "1.4.0",
    "husky": "7.0.1",
    "i18next": "20.4.0",
    "jest": "27.0.6",
    "lint-staged": "11.1.2",
    "lodash": "4.17.21",
    "prettier": "2.3.2",
    "prop-types": "15.7.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-i18next": "11.11.4",
    "rimraf": "3.0.2",
    "rollup": "2.56.2",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "rollup-plugin-typescript2": "0.30.0",
    "subscriptions-transport-ws": "0.9.19",
    "ts-jest": "27.0.4",
    "typescript": "4.3.5"
  },
  "peerDependencies": {
    "@types/lodash": "^4.14.164",
    "antd": "^4.16.7",
    "i18next": "^19.8.4 || ^20.0.0",
    "lodash": "^4.17.20",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-i18next": "^11.7.3",
    "subscriptions-transport-ws": "^0.9.18"
  },
  "lint-staged": {
    "*.(js|jsx|ts|tsx)": [
      "npm run lint:fix",
      "git add"
    ]
  }
}
