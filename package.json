{
  "name": "@fjedi/graphql-react-components",
  "version": "2.1.1",
  "description": "A set of helpers for managing apollo-graphql inside react-app",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "private": false,
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watchAll",
    "clean": "rimraf lib",
    "coverage": "jest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "build": "rollup -c",
    "start": "rollup -c -w",
    "format": "prettier --write \"src/**/*.tsx\"",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fjedi/graphql-react-components.git"
  },
  "keywords": [
    "fjedi",
    "graphql",
    "apollo",
    "react",
    "typescript"
  ],
  "author": "Alexander Radyushin <r.alex.andr@ya.ru> (https://fjedi.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fjedi/graphql-react-components/issues"
  },
  "homepage": "https://github.com/fjedi/graphql-react-components#readme",
  "dependencies": {
    "@apollo/client": "3.3.11",
    "@types/apollo-upload-client": "14.1.0",
    "@types/koa": "2.13.1",
    "apollo-cache-inmemory": "1.6.6",
    "apollo-cache-updater": "0.1.3",
    "apollo-link-debounce": "2.1.0",
    "apollo-link-persisted-queries": "0.2.2",
    "apollo-progressive-fragment-matcher": "1.0.8",
    "apollo-upload-client": "14.1.3",
    "apollo-utilities": "1.3.4",
    "graphql": "15.5.0"
  },
  "devDependencies": {
    "@types/expect": "24.3.0",
    "@types/jest": "26.0.20",
    "@types/lodash": "4.14.168",
    "@types/node": "14.14.32",
    "@types/react": "17.0.3",
    "@types/react-i18next": "8.1.0",
    "@typescript-eslint/eslint-plugin": "4.17.0",
    "@typescript-eslint/parser": "4.17.0",
    "antd": "4.13.1",
    "babel-core": "6.26.3",
    "babel-runtime": "6.26.0",
    "eslint": "7.21.0",
    "eslint-config-airbnb-typescript": "12.3.1",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-compat": "3.9.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-security": "1.4.0",
    "husky": "4.3.8",
    "i18next": "19.9.1",
    "jest": "26.6.3",
    "lint-staged": "10.5.4",
    "lodash": "4.17.21",
    "prettier": "2.2.1",
    "prop-types": "15.7.2",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-i18next": "11.8.8",
    "rimraf": "3.0.2",
    "rollup": "2.40.0",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "rollup-plugin-typescript2": "0.30.0",
    "subscriptions-transport-ws": "0.9.18",
    "ts-jest": "26.5.3",
    "typescript": "4.2.3"
  },
  "peerDependencies": {
    "@types/lodash": "^4.14.164",
    "antd": "^4.12.3",
    "i18next": "^19.8.4",
    "lodash": "^4.17.20",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-i18next": "^11.7.3",
    "subscriptions-transport-ws": "^0.9.18"
  },
  "lint-staged": {
    "*.(js|jsx|ts|tsx)": [
      "npm run lint:fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
